CREATE TABLE records (
  key TEXT NOT NULL,
  record TEXT NOT NULL,
  PRIMARY KEY (key)
);

recordForKey:
SELECT key, record FROM records WHERE key=?;

insert:
INSERT INTO records (key, record) VALUES (?,?);

update:
INSERT OR REPLACE INTO records (key, record) VALUES(?,?);

delete:
DELETE FROM records WHERE key=?;

changes:
SELECT changes();

deleteAll:
DELETE FROM records;
